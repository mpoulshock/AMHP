Cell[BoxData[
 RowBox[{
  RowBox[{"nashEquilibria", "[", 
   RowBox[{"utilTable_", ",", " ", "options_"}], "]"}], " ", ":=", " ", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"bestResponsesByPlayer", ",", "nashIDs", ",", " ", 
      RowBox[{"n", "=", 
       RowBox[{"options", "[", "\"\<n\>\"", "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{"(*", " ", 
     RowBox[{"Find", " ", "each", " ", 
      RowBox[{"player", "'"}], "s", " ", "best", " ", "responses"}], " ", 
     "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"bestResponsesByPlayer", " ", "=", " ", 
      RowBox[{"ParallelArray", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"bestResponsesForPlayer", "[", 
          RowBox[{
           RowBox[{"utilTable", "[", 
            RowBox[{"[", 
             RowBox[{"All", ",", "#"}], "]"}], "]"}], ",", 
           RowBox[{"options", "[", "\"\<breadth\>\"", "]"}], ",", "n", ",", 
           "#"}], "]"}], " ", "&"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Rows", " ", "that", " ", "are", " ", "Nash", " ", "equilibria"}], " ", 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nashIDs", " ", "=", " ", 
      RowBox[{"Apply", "[", 
       RowBox[{"Intersection", ",", "bestResponsesByPlayer"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "nashIDs", "]"}], ";"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
